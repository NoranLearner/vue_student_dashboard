{"ast":null,"code":"import useValidate from \"@vuelidate/core\";\nimport { required, email, minLength } from \"@vuelidate/validators\";\nexport default {\n  name: \"RegisterUsers\",\n  data() {\n    return {\n      errorMsg: \"\",\n      successMsg: \"\",\n      studentsData: [],\n      // showAddModal: false,\n      newStudent: {\n        name: \"\",\n        email: \"\",\n        phone: \"\"\n      },\n      v$: useValidate()\n    };\n  },\n  validations() {\n    return {\n      newStudent: {\n        name: {\n          required,\n          minLength: minLength(5)\n        },\n        email: {\n          required,\n          email\n        },\n        phone: {\n          required,\n          minLength: minLength(9)\n        }\n      }\n    };\n  },\n  methods: {\n    /* addModalDialog(val){\n        this.showAddModal = val;\n    }, */\n    addNewStudent() {\n      // console.log(\"Hello\");\n      this.v$.newStudent.$validate();\n      if (!this.v$.newStudent.$error) {\n        console.log(\"Add New Student : Validated Successfully\");\n      } else {\n        console.log(\"Add New Student : Not Validated\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAwJA,OAAOA,WAAU,MAAO,iBAAiB;AACzC,SAASC,QAAQ,EAAEC,KAAK,EAAEC,SAAQ,QAAS,uBAAuB;AAElE,eAAe;EAEXC,IAAI,EAAE,eAAe;EAErBC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,EAAE;MAChB;MACAC,UAAU,EAAE;QACRL,IAAI,EAAE,EAAE;QACRF,KAAK,EAAE,EAAE;QACTQ,KAAK,EAAE;MACX,CAAC;MACDC,EAAE,EAAEX,WAAW;IACnB;EACJ,CAAC;EAEDY,WAAW,GAAG;IACV,OAAO;MACHH,UAAU,EAAE;QACRL,IAAI,EAAE;UAACH,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAC,CAAC;QACzCD,KAAK,EAAE;UAACD,QAAQ;UAAEC;QAAK,CAAC;QACxBQ,KAAK,EAAE;UAAET,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAC;MAC9C;IACJ;EACJ,CAAC;EAEDU,OAAO,EAAE;IACL;;;IAGAC,aAAa,GAAG;MACZ;MACA,IAAI,CAACH,EAAE,CAACF,UAAU,CAACM,SAAS,EAAE;MAC9B,IAAI,CAAC,IAAI,CAACJ,EAAE,CAACF,UAAU,CAACO,MAAM,EAAE;QAC5BC,OAAO,CAACC,GAAG,CAAC,0CAA0C,CAAC;MAC3D,OAAO;QACHD,OAAO,CAACC,GAAG,CAAC,iCAAiC,CAAC;MAClD;IACJ;EACJ;AAEJ","names":["useValidate","required","email","minLength","name","data","errorMsg","successMsg","studentsData","newStudent","phone","v$","validations","methods","addNewStudent","$validate","$error","console","log"],"sourceRoot":"","sources":["/home/fedora/Downloads/student_dashboard/src/components/RegisterUsers.vue"],"sourcesContent":["<template>\n\n    <div>\n        <h1 class=\"text-center\"> CRUD PHP , Vuejs.3 & MySQL </h1>\n    </div>\n\n    <div class=\"container\">\n\n        <!-- Show Add Student Button -->\n        <div class=\"col-lg-12\">\n            <button class=\"btn btn-danger float-start\" v-if=\"studentsData.length > 0\">\n                <font-awesome-icon :icon=\"['fas', 'user-times']\" />\n                Delete All Students\n            </button>\n            <!-- @click=\"addModalDialog(true)\" -->\n            <button type=\"button\" class=\"btn btn-info float-end\" data-bs-toggle=\"modal\" data-bs-target=\"#overlay\">\n                <font-awesome-icon :icon=\"['fas', 'user']\" />\n                Add New Student\n            </button>\n            <div class=\"clearfix\"></div>\n        </div>\n\n        <hr class=\"bg-info\">\n\n        <!-- Show Error Message -->\n\n        <div class=\"alert alert-danger alert-dismissible fade show\" role=\"alert\" v-if=\"errorMsg\">\n            {{ errorMsg }}\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"close\"></button>\n        </div>\n\n        <!-- Show Success Message -->\n\n        <div class=\"alert alert-success alert-dismissible fade show\" role=\"alert\" v-if=\"successMsg\">\n            {{ successMsg }}\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"close\"></button>\n        </div>\n\n        <!-- Show Student Details : Tables -->\n\n        <table class=\"table table-border table-striped caption-top text-center\">\n\n            <caption>List of Registered Students ({{ studentsData.length }})</caption>\n\n            <thead>\n                <tr class=\"bg-success text-light\">\n                    <th>\n                        <font-awesome-icon :icon=\"['fas', 'id-badge']\" />\n                        ID\n                    </th>\n                    <th>\n                        <font-awesome-icon :icon=\"['fas', 'user']\" />\n                        Name\n                    </th>\n                    <th>\n                        <font-awesome-icon :icon=\"['fas', 'envelope']\" />\n                        Email\n                    </th>\n                    <th>\n                        <font-awesome-icon :icon=\"['fas', 'phone']\" />\n                        Phone\n                    </th>\n                    <th>\n                        <font-awesome-icon :icon=\"['fas', 'gear']\" />\n                        Actions\n                    </th>\n                </tr>\n            </thead>\n\n            <tbody>\n                <tr>\n                    <td>1</td>\n                    <td>nn</td>\n                    <td>salah@gmail.co</td>\n                    <td>123456789</td>\n                    <td>\n                        <button type=\"button\" class=\"btn btn-danger\">\n                            <font-awesome-icon :icon=\"['fas', 'user-xmark']\" />\n                            Delete\n                        </button>\n                        &nbsp;\n                        <button type=\"button\" class=\"btn btn-warning\">\n                            <font-awesome-icon :icon=\"['fas', 'user-pen']\" />\n                            Update\n                        </button>\n                    </td>\n                </tr>\n            </tbody>\n\n        </table>\n\n        <!-- Show Add New Student Modal -->\n\n        <!-- v-if=\"showAddModal\" -->\n        <div id=\"overlay\" class=\"modal fade\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-header\">\n                        <h5 class=\"modal-title text-info\" id=\"exampleModalLabel\">Add New Student</h5>\n                        <!-- @click=\"addModalDialog(false)\" -->\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                        <!-- <button aria-hidden=\"true\" @click=\"addModalDialog(false)\"><font-awesome-icon :icon=\"['fas', 'xmark']\" /></button> -->\n                    </div>\n                    <div class=\"modal-body p-4\">\n                        <form action=\"#\" method=\"post\" @click.prevent>\n                            <div class=\"form-floating mb-3\" :class=\"{ 'form-group-error': v$.newStudent.$error }\">\n                                <input type=\"text\" class=\"form-control\" id=\"StudentNameFloat\" placeholder=\"John Doe\"\n                                    v-model=\"newStudent.name\">\n                                <label for=\"StudentNameFloat\">\n                                    <font-awesome-icon :icon=\"['far', 'user']\" />\n                                    Student Name\n                                </label>\n                            </div>\n                            <div class=\"form-floating mb-3\">\n                                <input type=\"email\" class=\"form-control\" id=\"StudentEmailFloat\"\n                                    placeholder=\"John_Doe@gmail.com\" v-model=\"newStudent.email\">\n                                <label for=\"StudentEmailFloat\">\n                                    <font-awesome-icon :icon=\"['far', 'envelope']\" />\n                                    Student Email\n                                </label>\n                            </div>\n                            <div class=\"form-floating mb-3\">\n                                <input type=\"tel\" class=\"form-control\" id=\"StudentPhoneFloat\" placeholder=\"0123456789\"\n                                    v-model=\"newStudent.phone\">\n                                <label for=\"StudentPhoneFloat\">\n                                    <font-awesome-icon :icon=\"['fas', 'phone']\" />\n                                    Student Phone No.\n                                </label>\n                            </div>\n                            <hr class=\"bg-info\">\n                            <div class=\"d-grid gap-2\">\n                                <button type=\"submit\" class=\"btn btn-info\" @click=\"addNewStudent()\">Add New\n                                    Student</button>\n                            </div>\n                        </form>\n                    </div>\n                    <!-- <div class=\"modal-footer\">\n                        <button type=\"button\" class=\"btn btn-info\" data-bs-dismiss=\"modal\">Add New Student</button>\n                    </div> -->\n                </div>\n            </div>\n        </div>\n\n        <!-- Show Edit Student Modal -->\n        <!-- Show Delete Student Modal -->\n        <!-- Show Delete All Students Modal -->\n    </div>\n\n</template>\n\n<script>\n\nimport useValidate from \"@vuelidate/core\";\nimport { required, email, minLength } from \"@vuelidate/validators\";\n\nexport default {\n\n    name: \"RegisterUsers\",\n\n    data() {\n        return {\n            errorMsg: \"\",\n            successMsg: \"\",\n            studentsData: [],\n            // showAddModal: false,\n            newStudent: {\n                name: \"\",\n                email: \"\",\n                phone: \"\",\n            },\n            v$: useValidate(),\n        }\n    },\n\n    validations() {\n        return {\n            newStudent: {\n                name: {required, minLength: minLength(5)},\n                email: {required, email},\n                phone: { required, minLength: minLength(9)},\n            },\n        }\n    },\n\n    methods: {\n        /* addModalDialog(val){\n            this.showAddModal = val;\n        }, */\n        addNewStudent() {\n            // console.log(\"Hello\");\n            this.v$.newStudent.$validate();\n            if (!this.v$.newStudent.$error) {\n                console.log(\"Add New Student : Validated Successfully\");\n            } else {\n                console.log(\"Add New Student : Not Validated\");\n            }\n        },\n    },\n\n}\n</script>\n\n<style scoped lang=\"scss\">\nh1 {\n    background-color: cadetblue;\n    color: white;\n    padding: 8px;\n    margin-top: 0;\n    margin-bottom: 50px;\n}\n\n/* #overlay{\n    position: fixed;\n    top: 0;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    background-color: rgb(0, 0, 0, 0.3);\n} */\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}