789</td>\n                    <td>\n                        <button type=\"button\" class=\"btn btn-danger\">\n                            <font-awesome-icon :icon=\"['fas', 'user-xmark']\" />\n                            Delete\n                        </button>\n                        &nbsp;\n                        <button type=\"button\" class=\"btn btn-warning\">\n                            <font-awesome-icon :icon=\"['fas', 'user-pen']\" />\n                            Update\n                        </button>\n                    </td>\n                </tr>\n            </tbody>\n\n        </table>\n\n        <!-- Show Add New Student Modal -->\n\n        <div class=\"modal fade\" id=\"overlay\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-header\">\n                        <h5 class=\"modal-title text-info\" id=\"exampleModalLabel\">Add New Student</h5>\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                        <!-- <button aria-hidden=\"true\"><font-awesome-icon :icon=\"['fas', 'xmark']\" /></button> -->\n                    </div>\n                    <div class=\"modal-body p-4\">\n                        <form action=\"#\" method=\"post\" @click.prevent>\n                            <div class=\"form-floating mb-3\">\n                                <input type=\"text\" class=\"form-control\" id=\"StudentNameFloat\" placeholder=\"John Doe\">\n                                <label for=\"StudentNameFloat\" class=\"form-label\">\n                                    <font-awesome-icon :icon=\"['far', 'user']\" />\n                                    Student Name\n                                </label>\n                            </div>\n                            <div class=\"form-floating mb-3\">\n                                <input type=\"email\" class=\"form-control\" placeholder=\"Student Email\">\n                            </div>\n                            <!-- <button type=\"submit\" class=\"btn btn-primary\">Submit</button> -->\n                        </form>\n                    </div>\n                    <div class=\"modal-footer\">\n                        <button type=\"button\" class=\"btn btn-info\" data-bs-dismiss=\"modal\">Add New Student</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <!-- Show Edit Student Modal -->\n        <!-- Show Delete Student Modal -->\n        <!-- Show Delete All Students Modal -->\n    </div>\n\n</template>\n\n<script>\nexport default {\n    name: \"RegisterUsers\",\n    data() {\n        return {\n            errorMsg: \"\",\n            successMsg: \"\",\n            studentsData: [],\n        }\n    },\n}\n</script>\n\n<style scoped lang=\"scss\">\nh1{\n    background-color:cadetblue;\n    color: white;\n    padding: 8px;\n    margin-top: 0;\n    margin-bottom: 50px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}